    <sql>SELECT &#xd;&#xa;   wo.operation_type as operation_type,wo.created as created,wo.updated as updated,&#xd;&#xa;   wo.code as code, wo.description as desciption, wo.amount_tax as amount_tax,&#xd;&#xa;   wo.amount_with_tax as amount_with_tax, wo.amount_without_tax as amount_without_tax,&#xd;&#xa;   wo.end_date as end_date, wo.offer_code as offer_code, wo.operation_date as operation_date,&#xd;&#xa;   wo.quantity as quantity,wo.start_date as start_date,wo.status as status,&#xd;&#xa;   wo.subscription_date as subscription_date,wo.tax_percent as tax_percent,&#xd;&#xa;   p.code as provider_code, sub.code as sub_code, CURRENT_DATE as mrr_date, &#xd;&#xa;   wo.amount_without_tax&#x2f;&#x28;&#x28;EXTRACT&#x28;epoch FROM &#x28;wo.end_date - wo.start_date&#x29;&#x29;&#x2f;86400&#x29;&#x3a;&#x3a;int&#x29; as mrr_value,&#xd;&#xa;   s.code as seller_code,ua.code as ua_code,ba.code as ba_code,ca.code as ca_code,cust.code as cust_code,&#xd;&#xa;   wo.invoicing_date as invoicing_date&#xd;&#xa;FROM billing_wallet_operation wo &#xd;&#xa;LEFT JOIN crm_provider p ON p.id&#x3d;wo.provider_id&#xd;&#xa;LEFT JOIN billing_charge_instance ci ON ci.id&#x3d;wo.charge_instance_id&#xd;&#xa;LEFT JOIN billing_subscription sub ON sub.id &#x3d; ci.subscription_id&#xd;&#xa;LEFT JOIN billing_wallet w ON w.id&#x3d;wo.wallet_id&#xd;&#xa;LEFT JOIN crm_seller s ON s.id&#x3d;wo.seller_id&#xd;&#xa;LEFT JOIN billing_user_account uaa ON uaa.id&#x3d; w.user_account_id&#xd;&#xa;LEFT JOIN account_entity ua ON ua.id&#x3d; uaa.id&#xd;&#xa;LEFT JOIN billing_billing_account baa ON baa.id&#x3d; uaa.billing_account_id&#xd;&#xa;LEFT JOIN account_entity ba ON ba.id&#x3d; baa.id&#xd;&#xa;LEFT JOIN ar_customer_account caa ON caa.id&#x3d; baa.customer_account_id&#xd;&#xa;LEFT JOIN account_entity ca ON ca.id&#x3d; caa.id&#xd;&#xa;LEFT JOIN crm_customer custa ON custa.id&#x3d; caa.customer_id&#xd;&#xa;LEFT JOIN account_entity cust ON cust.id&#x3d; custa.id&#xd;&#xa;WHERE wo.disabled&#x3d;FALSE&#xd;&#xa;AND NOT&#x28;wo.end_date IS NULL&#x29;&#xd;&#xa;AND &#x28;&#x28;EXTRACT&#x28;epoch FROM &#x28;wo.end_date - wo.start_date&#x29;&#x29;&#x2f;86400&#x29;&#x3a;&#x3a;int&#x29;&#x3e;0&#xd;&#xa;AND &#x28;wo.start_date &#x3c;&#x3d; CURRENT_DATE&#x29; AND &#x28;wo.end_date &#x3e; CURRENT_DATE&#x29; </sql>
